{
  "name": "Web Slicer",
  "description": "",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["scripting"],
  "background": {
    "service_worker": "extension/background.js"
  },
  "content_scripts": [
    {
      "matches": "https://*.printables.com/*",
      "js": "extension/content.js"
    }
  ]
}